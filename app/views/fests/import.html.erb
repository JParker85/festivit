<h4>Import Fest Types</h4>
<%= form_tag import_fests_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
<% end %>
<br>
<h1>Current Fest Types</h1>

<table>
  <thead>
  <tr>
    <th>Fest id</th>
    <th>name</th>
    <th>Fest_code</th>
    <th>Tuple date</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @fests.each do |fest| %>
      <tr>
        <td><%= fest.id %></td>
        <td><%= fest.name %></td>
        <td><%= fest.fest_code %></td>
        <td><%= fest.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
