<h4>Import Ticket Types</h4>
<%= form_tag import_ticket_types_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
<% end %>

<p>
  Export Ticket Types:
  <%= link_to "CSV", ticket_types_path(format: "csv") %> |
  <%= link_to "Excel", ticket_types_path(format: "xls") %>
</p>



<br>

<h1>Current Ticket Types</h1>

<table>
  <thead>
  <tr>
    <th>TicketType id</th>
    <th>name</th>
    <th>Sale status</th>
    <th>Price</th>
    <th>SKU</th>
    <th>Fest ID</th>
    <th>ProductPairsID</th>
    <th>Tuple date</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @ticket_types.each do |ticket_type| %>
      <tr>
        <td><%= ticket_type.id %></td>
        <td><%= ticket_type.name %></td>
        <td><%= ticket_type.sale_status %></td>
        <td><%= ticket_type.price %></td>
        <td><%= ticket_type.sku %></td>
        <td><%= ticket_type.fest_id %></td>
        <td><%= ticket_type.productpairsid %></td>
        <td><%= ticket_type.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
