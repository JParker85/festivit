<h4>Import Role Types</h4>
<%= form_tag import_role_types_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
<% end %>

<br>

<h1>Current Role Types</h1>

<table>
  <thead>
  <tr>
    <th>RoleType id</th>
    <th>name</th>
    <th>Tuple date</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @role_types.each do |role_type| %>
      <tr>
        <td><%= role_type.id %></td>
        <td><%= role_type.name %></td>
        <td><%= role_type.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
